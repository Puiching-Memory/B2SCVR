{
    "seed": 2021,
    "save_dir": "release_models/release_model_b2scvr_bscvr_challenge_bs1_",
    "train_data_loader": {
        "name": "youtube-vos",
        "data_root": "/media/tianyi/BSC-VideoCompletion/baselines/BSCVR/datasets",
        "w": 432,
        "h": 240,
        "num_local_frames": 5,
        "num_ref_frames": 3
    },
    "losses": {
        "hole_weight": 10,
        "valid_weight": 10,
        "adversarial_weight": 0.01,
        "GAN_LOSS": "hinge"
    },
    "model": {
        "net": "bscvr_hq_p2_sam_v3",
        "no_dis": 0
    },
    "trainer": {
        "type": "Adam",
        "beta1": 0,
        "beta2": 0.99,
        "lr": 1e-4,
        "batch_size": 1,
        "num_workers": 1,
        "log_freq": 100,
        "save_freq": 5e3,
        "iterations": 100e3,
        "scheduler": {
            "type": "MultiStepLR",
            "milestones": [
                80e3
            ],
            "gamma": 0.1
        }
    }
}
